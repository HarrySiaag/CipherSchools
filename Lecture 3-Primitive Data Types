public class primitiveDT{

    public static void main(String[] args) {
        //integer data type
        int x = 5;
        int y = 7;
        System.out.println("X is : " + x);
        System.out.println("Y is : " + y);

        //byte data type
        byte b = 10;
        System.out.println("B is : " + b);

        //explicit conversion
        byte b2 = (byte)129;
        System.out.println("B2 is : " + b2);

        int i = b;
        System.out.println("I is : " + i);


    }
}
