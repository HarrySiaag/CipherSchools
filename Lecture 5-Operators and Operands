public class Operators_Operands{

    public static void main(String[] args) {
        

        //use of operators and operands
        float f = (float)5.5;

        float f2 = 5.5f; //by default it will be double

        double d = 10.5;

        char ch = 'a';
        System.out.println(ch);

        int z = x + y;  // '+' operator  x,y operands

        //Increment and decrement operators
        int m = 5;
        int n = m++; //post increment
        System.out.println("M is :" + m + ", N is :" + n);

        int p = 5;
        int q = ++p; //pre increment
        System.out.println("P is :" + p + ", Q is :" + q);


    }
}
